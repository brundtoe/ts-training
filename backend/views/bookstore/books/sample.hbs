<div class="content">
  <h3>Bøger i Bookstore</h3>
  <p class="heading">
    <!--suppress HtmlUnknownTarget -->
    <a class="btn btn-primary  btn-sm btn-link" href="/">Retur til Bookstore</a>
  </p>
  <p class="heading">Et udvalg af bøger på lager i Bookstore</p>


  <table id="books" class="table table-striped">
    <tbody data-testid="books">
    {{#each books}}
      <tr>
        <td>{{this.id}}</td>
        <td><a href="/api/books/{{this.id}}">{{this.title}}</a></td>
        <td>{{this.bookprice}}</td>
        <td>{{this.onhand}}</td>
      </tr>
    {{/each}}
    </tbody>
  </table>
</div>
